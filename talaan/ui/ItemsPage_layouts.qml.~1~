import QtQuick 2.4
import Ubuntu.Components 1.3
import QtQuick.Layouts 1.1
import Ubuntu.Layouts 1.0
import Ubuntu.Components.Themes.Ambiance 1.3
import QtQuick.Window 2.1
import "../components"
import "../components/Dialogs"
import "../library"
import Ubuntu.Components.ListItems 1.0 as ListItemOld
import Ubuntu.Components.Popups 1.3
import "../library/DataProcess.js" as DataProcess
import "../library/ProcessFunc.js" as Process

Page {
    id: pageItems
    //title: currentChecklist
    //flickable: null
    property string currentIndex
    property string currentDescr
    property string pageMode
    property string currentCategory
    property string currentChecklist
    property string currentStatus
    property string currentTargetDate
    property int currentID
    property var arrItemsStatus: []
    property int currentTotal: mainView.listItems.modelChecklistItems.count
    property int itemsTotal
    property int totalChecked
    property bool hideChecked: false

    header: PageHeader {
        title: currentChecklist
        //flickable: groupedList
        StyleHints {
            backgroundColor: "#3D1400"
            dividerColor: UbuntuColors.slate
        }
        contents: Label {
            id: lblHead
            text: currentChecklist
            fontSizeMode: Text.HorizontalFit
            fontSize: "large"
            minimumPixelSize: units.gu(2)
            elide: Text.ElideRight
            width: parent !== null ? parent.width : 0
            anchors.verticalCenter: parent.verticalCenter
            verticalAlignment: Text.AlignVCenter
        }
        extension: Rectangle {
            id: statsRec
            opacity: 0.8
            height: units.gu(4)
            width: parent.width
            color: theme.palette.normal.background
            visible: (pageMode === "talaan")
                     && currentStatus === "incomplete" ? true : false
            anchors {
                left: parent.left
                right: parent.right
                bottom: parent.bottom
            }
            Label {
                anchors {
                    verticalCenter: parent.verticalCenter
                    right: parent.right
                    rightMargin: units.gu(2)
                }
                //color: UbuntuColors.green
                horizontalAlignment: Text.AlignRight

                text: "Total: " + String(itemsTotal)
            }
            Label {
                anchors {
                    verticalCenter: parent.verticalCenter
                    leftMargin: units.gu(2)
                    left: parent.left
                }
                //color: UbuntuColors.blue
                text: "Remaining: " + String(currentTotal - totalChecked)
            }
        }

        trailingActionBar {
            numberOfSlots: mainLayout.multiColumn ? 3 : 2
            actions: [
                Action {
                    iconName: "add"
                    text: i18n.tr("Add")
                    visible: mainLayout.columns === 1 ? false : true
                    onTriggered: {
                        addNew()
                    }
                },
                Action {
                    iconName: hideChecked === false ? "empty-symbolic" : "text-x-generic-symbolic"
                    text: hideChecked ? i18n.tr("Show All") : i18n.tr(
                                            "Hide Checked")
                    enabled: checkComplete() === true ? false : true
                    visible: pageMode === "talaan" ? true : false
                    onTriggered: {
                        hideChecked = !hideChecked
                    }
                },
                Action {
                    iconName: "select"
                    text: i18n.tr("Mark as completed")
                    visible: pageMode === "talaan"
                             && mainView.listItems.modelChecklistItems.count > 0 ? true : false
                    onTriggered: {
                        completeChecklist("")
                    }
                },
                Action {
                    iconName: "select-none"
                    text: i18n.tr("Uncheck All")
                    visible: pageMode === "talaan"
                             && mainView.listItems.modelChecklistItems.count > 0 ? true : false
                    onTriggered: {
                        uncheckAll()
                    }
                },
                Action {
                    iconName: "note-new"
                    text: i18n.tr("New checklist")
                    visible: pageMode === "favorite" ? true : false
                    onTriggered: {
                        var newChecklist
                        newChecklist = DataProcess.createNewFromFavorite(
                                    currentID)
                        mainView.notification.showNotification(
                                    i18n.tr("New List successfully created"),
                                    UbuntuColors.green)
                        pageStack.pop()
                        tabs.selectedTabIndex = defaultTab.index
                        mainView.mainPageStack.push(itemsPage, {
                                                        pageMode: "talaan",
                                                        currentCategory: newChecklist.category,
                                                        currentChecklist: newChecklist.checklist,
                                                        currentID: newChecklist.id,
                                                        currentStatus: newChecklist.status,
                                                        currentTotal: newChecklist.total,
                                                        currentDescr: newChecklist.descr
                                                    })
                    }
                },
                Action {
                    iconName: "edit"
                    text: i18n.tr("Edit")
                    visible: pageMode !== "history" ? true : false
                    onTriggered: {
                        pageStack.pop()
                        pageStack.push(Qt.resolvedUrl("AddChecklist.qml"), {
                                           mode: "edit",
                                           checklistID: currentID,
                                           type: currentStatus,
                                           checklist: currentChecklist,
                                           category: currentCategory,
                                           description: currentDescr,
                                           targetDate: currentTargetDate
                                       })
                    }
                },
                Action {
                    iconName: "info"
                    text: i18n.tr("View Details")
                    onTriggered: {
                        PopupUtils.open(infoDialog)
                    }
                }
            ]
        }
    }

    Component {
        id: infoDialog
        Dialog {
            id: infoDialogue
            title: currentChecklist
            text: currentDescr //mainView.listItems.modelChecklist.get(currentIndex).descr

            Button {
                text: i18n.tr("Close")
                color: UbuntuColors.warmGrey
                onClicked: {
                    PopupUtils.close(infoDialogue)
                }
            }
        }
    }
    onHideCheckedChanged: {
        if (hideChecked === true) {
            loadChecklist(0) // load only unchecked items
        } else {
            loadChecklist(null) //load all items
        }
    }

    Loader {
        id: listViewLoader
    }

    //functions
    function addNew() {
        bottomEdge.visible = true
        bottomEdge.mode = "add"
        bottomEdge.edgeState = "expanded"
    }

    function loadChecklist(status) {


        //        0 = Unchecked
        //        1 = Checked
        //        2 = Skipped
        mainView.listItems.modelChecklistItems.getItems(currentID, status)
        getCheckedCount()
        currentTotal = mainView.listItems.modelChecklistItems.count
    }

    function checkEmpty() {
        if (mainView.listItems.modelChecklistItems.count === 0) {
            emptyState.visible = true
        } else {
            emptyState.visible = false
        }
    }

    function clearFields() {
        textItemName.text = ""
        textareaComment.text = ""
    }

    function getCheckedCount() {
        var modelItems = mainView.listItems.modelChecklistItems

        totalChecked = 0
        for (var i = 0; i < modelItems.count; i++) {
            //assign values to the array
            if (modelItems.get(i).status === 1 || modelItems.get(
                        i).status === 2) {
                totalChecked++
            }
        }
    }

    function checkComplete() {
        console.log("Checked: " + totalChecked + ", Total: " + itemsTotal)
        if (currentTotal - totalChecked === 0) {
            //(itemsTotal === totalChecked) {
            return true
        } else {
            return false
        }
    }

    function getItemModelIndex(itemName, searchModel) {
        var boolItemFound = false
        var i = 0
        var modelCount = searchModel.count
        while (i < modelCount) {
            if (searchModel.get(i).itemName === itemName) {
                boolItemFound = true
                break
            }
            i++
        }
        return i
    }

    function uncheckAll() {

        var dialogCompleted = PopupUtils.open(dialogUncheckAll)

        var continueUncheck = function () {
            DataProcess.uncheckAllItems(currentID)
            loadChecklist(null)
            hideChecked = false
        }

        dialogCompleted.proceed.connect(continueUncheck)
    }

    function completeChecklist(lastItemIndex) {

        var dialogCompleted
        if (lastItemIndex !== "") {
            dialogCompleted = PopupUtils.open(dialog)
        } else {
            dialogCompleted = PopupUtils.open(dialogMarkComplete)
        }

        var toggleCheckbox = function () {
            dialogCompleted.undo.disconnect(toggleCheckbox)
            groupedList.changeStatus(0, lastItemIndex)
        }
        var continueToHistory = function () {
            dialogCompleted.proceed.disconnect(toggleCheckbox)
            DataProcess.updateChecklistComplete(currentID)
            pageStack.pop()
            mainView.notification.showNotification(
                        "Checklist completed successfully",
                        theme.palette.normal.positive)
        }

        dialogCompleted.undo.connect(toggleCheckbox)
        dialogCompleted.proceed.connect(continueToHistory)
    }

    onActiveChanged: {

        if (active === true) {
            mainView.anchorToKeyboard = false
            totalChecked = 0
            loadChecklist(null)
            itemsTotal = mainView.listItems.modelChecklistItems.count
            checkEmpty()
        } else {
            mainView.anchorToKeyboard = true
            totalChecked = 0
            bottomEdge.edgeState = "collapsed"
            hideChecked = false
        }
    }

    EmptyState {
        id: emptyState
        iconName: "stock_note"
        title: i18n.tr("List is Empty")
        subTitle: i18n.tr("Swipe from the bottom to add items")
        anchors.centerIn: parent
        shown: false
    }
    PageBackGround {
        id: pageBackground
    }

    ListItemActions {
        id: leading

        actions: Action {
            iconName: "delete"
            text: i18n.tr("Delete")
            onTriggered: {
                var currentItemName = groupedList.model.get(value).itemName
                DataProcess.deleteItem(groupedList.currentItem.itemID,
                                       currentItemName)
                groupedList.model.remove(value)
                getCheckedCount()
                itemsTotal--
            }
        }
    }

    ListItemActions {
        id: trailing
        actions: [
            Action {
                iconName: "edit"
                text: i18n.tr("Edit")
                //visible: pageMode === "history"? false : true
                onTriggered: {
                    var currentItemName = groupedList.model.get(value).itemName
                    var currentItemComment = groupedList.model.get(
                                value).comments

                    bottomEdge.mode = "edit"
                    bottomEdge.currentItem = currentItemName
                    bottomEdge.currentComment = currentItemComment
                    bottomEdge.edgeState = "expanded"
                }
            },
            Action {
                iconName: "redo"
                text: i18n.tr("Skip")
                visible: pageMode === "talaan"
                         || pageMode === "normal" ? true : false
                onTriggered: {
                    groupedList.changeStatus(2, value)
                    if (checkComplete() === true) {
                        completeChecklist(value)
                    }
                }
            }
        ]
    }

    Layouts {
        id: itemsListLayouts
        anchors.fill: parent
        anchors.topMargin: pageItems.header.height

        layouts: [
            ConditionalLayout {
                name: "grid"
                when: itemsListLayouts.width > units.gu(60)
                ScrollView {
                    anchors.fill: parent
                    GridView {

                        id: groupedList
                        anchors {
                            fill: parent
                        }
                        interactive: true
                        model: mainView.listItems.modelChecklistItems
                        //clip: true
                        cellHeight: units.gu(4)
                        cellWidth: parent.width / 2

                        ItemLayout {
                            item: "commentBubble"
                        }

                        delegate:    ListItemContent{}//Label{text: itemName}
                    }
                }
            }
        ]

        ScrollView {
            anchors.fill: parent

            //anchors.topMargin: pageItems.header.height
            ListView {
                id: groupedList

                anchors {
                    fill: parent
                }
                interactive: true
                model: mainView.listItems.modelChecklistItems
                clip: true
                currentIndex: -1

                MessageBubble {
                    id: commentBubble
                    Layouts.item: "commentBubble"
                    showDuration: 0
                }

                function changeStatus(newStatus, itemIndex) {

                    var currentItemName = groupedList.model.get(
                                itemIndex).itemName
                    var i = getItemModelIndex(
                                currentItemName,
                                mainView.listItems.modelChecklistItems)
                    mainView.listItems.modelChecklistItems.setProperty(
                                i, "status", newStatus)
                    DataProcess.updateItemStatus(currentID, currentItemName,
                                                 newStatus)
                    getCheckedCount()
                }

                delegate: ListItem {
                    id: listWithActions
                    height: labelName.height + units.gu(4)
                    width: itemsListLayouts.currentLayout === "grid" ? parent.width * 0.4 : parent.width
                    color: "transparent"
                    highlightColor: "#2D371300"
                    Layouts.item: "listItem"
                    property int currentIndex: index
                    property string itemID: currentID

                    onClicked: {
                        if (pageMode === "talaan") {
                            groupedList.changeStatus(status === 0 ? 1 : 0,
                                                                    index)
                            if (checkItem.checked) {
                                if (checkComplete() === true) {
                                    completeChecklist(index)
                                }
                            }
                        }
                    }
                    leadingActions: pageMode === "history" ? null : leading
                    trailingActions: pageMode === "history" ? null : trailing

                    ListItemContent{}
                }
                Component {
                    id: dialog
                    Dialog {
                        id: dialogue
                        title: "Checklist Completed"
                        text: "You can view completed checklists from the History tab."
                        signal undo
                        signal proceed

                        Button {
                            text: "Continue"
                            color: UbuntuColors.green
                            onClicked: {
                                dialogue.proceed()
                                PopupUtils.close(dialogue)
                            }
                        }
                        Button {
                            color: UbuntuColors.red
                            text: i18n.tr("Undo last item")
                            onClicked: {
                                dialogue.undo()
                                PopupUtils.close(dialogue)
                            }
                        }
                    }
                }

                Component {
                    id: dialogMarkComplete
                    Dialog {
                        id: dialogue
                        title: "Mark this checklist as completed?"
                        text: "You can view completed checklists from the History tab."
                        signal undo
                        signal proceed

                        Button {
                            text: "Continue"
                            color: theme.palette.normal.positive
                            onClicked: {
                                dialogue.proceed()
                                PopupUtils.close(dialogue)
                            }
                        }
                        Button {
                            color: theme.palette.normal.negative
                            text: i18n.tr("Cancel")
                            onClicked: {
                                dialogue.undo()
                                PopupUtils.close(dialogue)
                            }
                        }
                    }
                }

                Component {
                    id: dialogUncheckAll
                    Dialog {
                        id: dialogue
                        text: "Are you sure to uncheck all items? Current state of each items won't be recoverable anymore."
                        signal undo
                        signal proceed

                        Button {
                            text: "Uncheck All"
                            color: theme.palette.normal.base
                            onClicked: {
                                dialogue.proceed()
                                PopupUtils.close(dialogue)
                            }
                        }
                        Button {
                            color: theme.palette.normal.negative
                            text: i18n.tr("Cancel")
                            onClicked: {
                                dialogue.undo()
                                PopupUtils.close(dialogue)
                            }
                        }
                    }
                }
            }
        }
    }

    DialogBottomEdge {
        id: bottomEdge

        property string mode: "add"
        property string currentItem
        property string currentComment

        hintColor: "#513838" //"#513838"
        hintIconName: "add"
        hintIconColor: "white"
        bgColor: "black"
        bgOpacity: 0.7
        actionButtonDistance: units.gu(5)
        dialogHeight: dialogAction.dialogHeight

        visible: !mainLayout.multiColumn ? (pageMode === "talaan"
                                            && currentStatus === "incomplete")
                                           || pageMode === "favorite"
                                           || pageMode === "normal" ? true : false : false

        onIsExpanded: {
            textItemName.forceActiveFocus()
        }

        onEdgeStateChanged: {
            if (edgeState === "expanded") {
                textItemName.enabled = true
                textareaComment.enabled = true
                hintColor = UbuntuColors.red
                hintIconName = "go-down"
                //textItemName.forceActiveFocus();
                if (bottomEdge.mode === "edit") {
                    //pageItems.title = "Edit " + currentItem
                    textItemName.text = currentItem
                    textareaComment.text = currentComment
                } else {


                    //pageItems.title = "Add an Item"
                }
            } else if (edgeState === "collapsed") {
                textItemName.enabled = false
                textareaComment.enabled = false
                visible = mainLayout.multiColumn ? false : true
                hintColor = "#513838" //UbuntuColors.green
                hintIconName = "add"
                //pageItems.title = currentChecklist
                if (bottomEdge.mode === "edit") {
                    bottomEdge.mode = "add"
                    textItemName.text = ""
                    textareaComment.text = ""
                }
                columnDialog.forceActiveFocus() //remove focus to any element
            }
        }

        leftAction: RadialAction {
            iconName: "reset"
            iconColor: "white"
            backgroundColor: UbuntuColors.warmGrey
            onTriggered: {
                console.log("Reset")
                textItemName.text = ""
                textareaComment.text = ""
            }
        }

        rightAction: RadialAction {
            iconName: bottomEdge.mode === "add" ? "add" : "save"
            iconColor: "white"
            backgroundColor: UbuntuColors.green
            onTriggered: {
                console.log("Save")
                var txtName = textItemName.text
                var txtComment = textareaComment.text

                if (Process.checkRequired([txtName]) === false) {
                    textItemName.forceActiveFocus()
                } else if (bottomEdge.currentItem !== txtName
                           && DataProcess.itemExist(currentID,
                                                    txtName) === true) {
                    mainView.notification.showNotification(
                                "Item name already exists!", UbuntuColors.red)
                } else {
                    switch (bottomEdge.mode) {
                    case "add":
                        DataProcess.saveItem(currentID, currentChecklist,
                                             txtName, txtComment)
                        mainView.notification.showNotification(
                                    "Item added successfully",
                                    UbuntuColors.green)
                        textItemName.forceActiveFocus()
                        itemsTotal++
                        break
                    case "edit":
                        DataProcess.updateItem(currentID,
                                               bottomEdge.currentItem, txtName,
                                               txtComment)
                        bottomEdge.edgeState = "collapsed"
                        mainView.notification.showNotification(
                                    "Item updated successfully",
                                    UbuntuColors.green)
                        break
                    }
                    textItemName.text = ""
                    textareaComment.text = ""
                    loadChecklist(hideChecked === true ? 0 : null)
                    checkEmpty()
                }
            }
        }

        contents: DialogAction {
            id: dialogAction
            property real dialogHeight: recContainer.height

            Rectangle {
                id: recContainer
                color: "#50362D" //"#331A00"
                radius: units.gu(2)
                width: textareaComment.width + units.gu(5)
                height: columnDialog.childrenRect.height + units.gu(
                            6) //textareaComment.height * 2 + units.gu(6)
                //opacity: 0.7
            }
            ColumnLayout {
                id: columnDialog

                //layoutDirection: Qt.RightToLeft
                spacing: units.gu(2)
                //anchors.centerIn: recContainer
                anchors.horizontalCenter: recContainer.horizontalCenter
                y: recContainer.y + units.gu(3)

                TextField {
                    id: textItemName
                    placeholderText: i18n.tr("Enter Item name")
                    Layout.alignment: Qt.AlignHCenter
                    anchors {
                        left: textareaComment.left
                        right: textareaComment.right
                    }

                    /*style: TextFieldStyle {
                                                                                                                                                                                                                                                                                                        //overlaySpacing: 0
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                